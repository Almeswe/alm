function mod(integer num,integer modnum) of integer
{
	integer r = num;
	while (r > modnum)
	{
		r = r-modnum;		
	}
	if (r==modnum){ return 0; }
	return r;
}

function isSimple(integer number) of boolean
{	
	if (number == 1 or number == 0) { return false; }

	integer i = 1;

	while (i < number)
	{
		if (mod(number,i) == 0)
		{
			if (i != 1 and i != number) { return false; }
		}
		i = i+1;
	}

	return true;
}


function factorial(integer n) of integer
{
	if (n < 0) { return 1; }
	if (n == 0 or n == 1) { return 1; }
	integer i = 1;
	integer res = 1;
	while (i < n+1)
	{
		res = res * i;
		i = i+1;
	}
	return res;
}