{
	return Round(a);
}
func sign(float a) : integer
{
	return Sign(a);
}
func sin(float value) : float
{
	return Sin(value);
}
func sinh(float value) : float
{
	return value;
}
func sqrt(float value) : float
{
	return Sqrt(value);
}
func tan(float value) : float
{
