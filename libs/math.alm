}
func tanh(float value) : float
{
	return Tanh(value);
}
func trunc(float value) : float
{
	return Truncate(value);
}
func log(float a, float newBase) : float
{
	return Log(a, newBase);
}
func lg(float d) : float
{
	return Log(d, 10.0);
}
func ln(float d) : float
{
	return Log(d, e());
}

